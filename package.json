{
  "name": "breakdancer",
  "version": "1.2.0",
  "description": "A breakpoint tracking utility",
  "main": "./lib",
  "browser": "./index",
  "scripts": {
    "lint": "eslint-godaddy *.js",
    "test:web": "mocha --compilers js:babel-register ./test.js",
    "test:native": "mocha --require react-native-mock/mock.js --compilers js:babel-register ./test.native.js",
    "build": "babel index.js index.native.js breakdancer.js -d ./lib",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "test": "npm run test:web && npm run test:native"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/breakdancer.git"
  },
  "keywords": [
    "breakpoints",
    "breakdancer",
    "breakpoint",
    "tracking",
    "break",
    "point"
  ],
  "license": "MIT",
  "devDependencies": {
    "assume": "^1.5.2",
    "babel-cli": "^6.26.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.2.0",
    "eslint-config-godaddy": "^2.0.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^4.0.0",
    "pre-commit": "^1.2.2",
    "react": "^15.6.1",
    "react-native": "^0.46.1",
    "react-native-mock": "^0.3.1"
  },
  "pre-commit": "lint, test",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.0",
    "babelify": "^7.3.0",
    "propget": "^1.1.0"
  },
  "babel": {
    "presets": [
      "babel-preset-es2015"
    ]
  },
  "peerDependencies": {
    "react": "^15.6.1"
  }
}
