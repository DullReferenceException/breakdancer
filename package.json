{
  "name": "breakdancer",
  "version": "1.0.1",
  "description": "A breakpoint tracking utility",
  "main": "./lib",
  "browser": "./src",
  "scripts": {
    "test": "mocha --compilers js:babel-register ./test.js",
    "lint": "godaddy-js-style-lint ./src ./test.js",
    "build": "babel ./src -d ./lib",
    "pretest": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/breakdancer.git"
  },
  "keywords": [
    "breakpoints",
    "breakdancer",
    "breakpoint",
    "tracking",
    "break",
    "point"
  ],
  "license": "MIT",
  "devDependencies": {
    "assume": "1.4.x",
    "babel-cli": "~6.18.0",
    "babel-register": "6.9.x",
    "godaddy-style": "3.1.x",
    "mocha": "2.5.x",
    "pre-commit": "1.1.x"
  },
  "pre-commit": "lint, test",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "babel-preset-es2015": "6.9.x",
    "babelify": "7.3.x",
    "propget": "1.0.x"
  },
  "babel": {
    "presets": [
      "babel-preset-es2015"
    ]
  }
}
